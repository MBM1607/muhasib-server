{
	"name": "muhasib-server",
	"version": "1.0.0",
	"author": {
		"name": "Muhammad Khan",
		"email": "me@muhammadkhan.dev",
		"url": "https://muhammadkhan.dev"
	},
	"license": "MIT",
	"type": "module",
	"scripts": {
		"dev": "tsx watch --env-file=.env src/app.ts",
		"build": "rimraf ./build && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
		"start": "node --experimental-specifier-resolution=node build/app.js",
		"lint": "eslint . --fix --report-unused-disable-directives && pnpm format && pnpm spellcheck",
		"format": "prettier --write --log-level silent .",
		"spellcheck": "cspell . --quiet",
		"test": "vitest watch",
		"test-ui": "vitest --ui",
		"typecheck": "vitest typecheck",
		"env:new": "dotenv-vault new -y",
		"env:login": "dotenv-vault login -y",
		"env:push": "dotenv-vault push",
		"env:pull": "dotenv-vault pull",
		"env:open": "dotenv-vault open -y",
		"keygen": "ssh-keygen -t rsa -b 4096 -m PEM -f .rsa.key && openssl rsa -in .rsa.key -pubout -outform PEM -out .rsa.key.pub",
		"trace": "tsc --generateTrace ./trace && npx @typescript/analyze-trace trace",
		"generate": "drizzle-kit generate:sqlite"
	},
	"dependencies": {
		"@libsql/client": "^0.3.6",
		"@ts-rest/core": "^3.45.2",
		"@ts-rest/express": "^3.45.2",
		"cors": "^2.8.5",
		"dayjs": "^1.11.11",
		"drizzle-orm": "^0.28.6",
		"drizzle-zod": "^0.5.1",
		"express": "^4.19.2",
		"helmet": "^7.1.0",
		"jsonwebtoken": "^9.0.2",
		"pino": "^8.21.0",
		"pino-http": "^8.6.1",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@flydotio/dockerfile": "^0.4.11",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/cors": "^2.8.17",
		"@types/eslint": "^8.56.10",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/node": "^20.14.6",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@typescript/analyze-trace": "^0.10.1",
		"@vitest/coverage-v8": "^0.34.6",
		"@vitest/ui": "^0.34.7",
		"cspell": "^7.3.9",
		"dotenv-vault": "^1.26.2",
		"drizzle-kit": "^0.19.13",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-unused-imports": "^3.2.0",
		"eslint-plugin-vitest": "^0.3.26",
		"pino-pretty": "^10.3.1",
		"prettier": "^3.3.2",
		"rimraf": "^5.0.7",
		"tsc-alias": "^1.8.10",
		"tsx": "^3.14.0",
		"typescript": "^5.4.5",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^0.34.6"
	},
	"packageManager": "pnpm@9.4.0+sha512.f549b8a52c9d2b8536762f99c0722205efc5af913e77835dbccc3b0b0b2ca9e7dc8022b78062c17291c48e88749c70ce88eb5a74f1fa8c4bf5e18bb46c8bd83a"
}
